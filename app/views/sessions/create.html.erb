<p id="alert"><%= alert %></p>

<main id="signup">
  <div class="signup-container">
    <h1>Signup</h1>
    <%= form_tag sessions_path do |form| %>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <%= submit_tag "Login" %>
    <% end %>
  </div>
</main>