{
  "name": "docify",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "ppx-flags": ["graphql_ppx/ppx"],
  "reason": {
    "react-jsx": 2
  },
  "namespace": true,
  "bs-dependencies": ["reason-react", "reason-apollo", "bs-css"],
  "sources": [
    {
      "dir": "app/javascript/",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}
